RewriteRule ^admin/photo_galleries/delete/([^/.]+)$     admin/index.php?page=photo_galleries&action=delete_gallery&gallery_ID=$1

RewriteRule ^admin/content/([^/.]+)$     admin/index.php?page=$1
RewriteRule ^admin/content/([^/.]+)/([^/.]+)$     admin/index.php?page=$1&action=$2
RewriteRule ^admin/content/([^/.]+)/([^/.]+)/([^/.]+)$     admin/index.php?page=$1&action=$2&ID=$3

RewriteRule ^admin/news/([^/.]+)$     admin/index.php?page=$1
RewriteRule ^admin/news/([^/.]+)/([^/.]+)$     admin/index.php?page=$1&action=$2
RewriteRule ^admin/news/([^/.]+)/([^/.]+)/([^/.]+)$     admin/index.php?page=$1&action=$2&ID=$3

RewriteRule ^admin/edit_article/([^/.]+)$     admin/index.php?page=edit_article&ID=$1
