RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.homebuilthifi.com/$1 [R,L]

RewriteRule ^admin$     admin/index.php
RewriteRule ^admin/$     admin/index.php

RewriteRule ^admin/content/edit_page/([^/.]+)$     admin/index.php?page=edit_page&ID=$1
RewriteRule ^admin/content/edit_secure_page/([^/.]+)$     admin/index.php?page=edit_secure_page&ID=$1
RewriteRule ^admin/content/edit_section/([^/.]+)$     admin/index.php?page=edit_section&ID=$1

RewriteRule ^admin/edit_project/([^/.]+)$     admin/index.php?page=edit_project&ID=$1
RewriteRule ^admin/edit_project_images/([^/.]+)$     admin/index.php?page=edit_project_images&ID=$1

RewriteRule ^admin/edit_comment/([^/.]+)$     admin/index.php?page=edit_comment&ID=$1
RewriteRule ^admin/delete_comment/([^/.]+)$     admin/index.php?page=comments&action=delete_comment&ID=$1

RewriteRule ^admin/image_manager/([^/.]+)$     admin/index.php?page=image_manager&folder=$1
RewriteRule ^admin/file_manager/([^/.]+)$     admin/index.php?page=file_manager&folder=$1
RewriteRule ^admin/menu_editor/([^/.]+)$     admin/index.php?page=menu_editor&menu_id=$1
