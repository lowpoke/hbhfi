RewriteRule ^admin/user_accounts/delete/([^/.]+)$     admin/index.php?page=user_accounts&action=delete_user&ID=$1

RewriteRule ^admin/projects/all$     admin/index.php?page=projects&view=all
RewriteRule ^admin/projects/([^/.]+)/([^/.]+)$     admin/index.php?page=projects&view=$1&view_by=$2

RewriteRule ^admin/builders/all$     admin/index.php?page=builders&view=all
RewriteRule ^admin/builders/([^/.]+)/([^/.]+)$     admin/index.php?page=builders&view=$1&view_by=$2

RewriteRule ^admin/([^/.]+)$     admin/index.php?page=$1

RewriteRule ^404$     index.php?page=404
RewriteRule ^news$     index.php?page=news
RewriteRule ^news/page/([^/.]+)$     index.php?page=news&page_num=$1
RewriteRule ^news/category/([^/.]+)$     index.php?page=news&category=$1
RewriteRule ^news/([^/.]+)$     index.php?page=news&article_url=$1

RewriteRule ^([^/.]+)/page/([^/.]+)$     index.php?page=$1&projects_page=$2

RewriteRule ^project/([^/.]+)$     index.php?page=project&project_id=$1
RewriteRule ^projects/builder/([^/.]+)$     index.php?page=projects&view_by=builder&builder=$1
RewriteRule ^projects/popular$     index.php?page=projects&project_category=popular
RewriteRule ^builder/([^/.]+)$     index.php?page=builder&builder_id=$1
RewriteRule ^builder/([^/.]+)/page/([^/.]+)$     index.php?page=builder&builder_id=$1&projects_page=$2

RewriteRule ^submit/([^/.]+)$     index.php?page=submit&step=$1

RewriteRule ^update/([^/.]+)$     index.php?page=update&project_id=$1&section=details
RewriteRule ^update/([^/.]+)/images$     index.php?page=update&project_id=$1&section=images
RewriteRule ^update/([^/.]+)/confirm$     index.php?page=update&project_id=$1&section=confirm
RewriteRule ^update/([^/.]+)/image_delete/([^/.]+)$     index.php?page=update&project_id=$1&section=image_delete&image_id=$2
RewriteRule ^update/([^/.]+)/thumbnail$     index.php?page=update&project_id=$1&section=thumbnail
RewriteRule ^update/([^/.]+)/youtube$     index.php?page=update&project_id=$1&section=youtube
RewriteRule ^update/save$     index.php?page=update

RewriteRule ^search$     index.php?page=projects&mode=search

RewriteRule ^secureimage$     functions/securimage/securimage_show.php

RewriteRule ^([^/.]+)/([^/.]+)$     index.php?page=$2&section=$1
RewriteRule ^([^/.]+)/$     index.php?page=$1
RewriteRule ^([^/.]+)$     index.php?page=$1

DirectoryIndex index.php

